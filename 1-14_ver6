# 라이브러리 불러오기

install.packages("data.table")
library(dplyr)
library(ggplot2)
library(data.table)


# 데이터 로드

product <- fread("Product.csv", 
                 header=TRUE,
                 encoding="UTF-8",
                 stringsAsFactors = FALSE)

custom <- fread("Custom.csv", 
                header=TRUE,
                encoding="UTF-8",
                stringsAsFactors = FALSE)

session <- fread("Session.csv", 
                 header=TRUE,
                 encoding="UTF-8",
                 stringsAsFactors = FALSE)

master <- fread("Master.csv", 
                header=TRUE,
                encoding="UTF-8",
                stringsAsFactors = FALSE)

search1 <- fread("Search1.csv", 
                 header=TRUE,
                 encoding="UTF-8",
                 stringsAsFactors = FALSE)

search2 <- fread("Search2.csv", 
                 header=TRUE,
                 encoding="UTF-8",
                 stringsAsFactors = FALSE)


