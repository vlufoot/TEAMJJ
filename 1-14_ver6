hist(custom$CLNT_AGE)         

custom$CLNT_GENDER <- as.factor(custom$CLNT_GENDER)   # 성별 팩터타입 변환
custom$CLNT_AGE <- as.factor(custom$CLNT_AGE)         # 나이 팩터타입 변환

table(is.na(custom$CLNT_ID))
table(is.na(custom$CLNT_GENDER))
